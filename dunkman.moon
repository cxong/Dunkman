		cls(0)
		drawimage(slam_values,slam_runs,WIDTH,HEIGHT,0,0)

class HoopState extends TimedState
	new:=>
		super 200

	draw:=>
		cls(0)
		drawimage(hoop_values,hoop_runs,WIDTH,HEIGHT,0,0)

class GameState extends TimedState
	new:=>
		super MUSLEN

	reset:=>
		music(MUSGAME,-1,-1,true)
		@ball=Ball!
		@handgen=HandGen!
		@lines=Speedlines!

	update:=>
		super!
		@ball\update!
		@handgen\update(@tt)
		handonball=@handgen\gethandonball(@ball)
		if handonball
			sfx(SFXHIT)
		@handgen\postupdate(@ball)
		@lines\update!

	finish:=>
		music!

	draw:=>
		-- draw speed lines
		@lines\draw!
		drawimage(bg_values,bg_runs,WIDTH,HEIGHT,0,0)
		@ball\draw!
		@handgen\draw!
		print("DUNKING: #{@tt*1000//MUSLEN/10}%",84,12,12)

titlestate=TitleState!
gamestate=GameState!
hoopstate=HoopState!
slamstate=SlamState!
titlestate.nextstate=gamestate
gamestate.nextstate=hoopstate
hoopstate.nextstate=slamstate
slamstate.nextstate=titlestate
state=titlestate
state\reset!

export TIC=->

	state\update!
	state\draw!
	state=state\next!

