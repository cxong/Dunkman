-- title:  Dunkman
-- author: congusbongus
-- desc:   Get past the defenders for a slam dunk
-- script: moon

WIDTH=240
CENTERX=WIDTH/2
HEIGHT=136
CENTERY=HEIGHT/2

SFXHIT=10

outquad = (t) -> return 1-(1-t)*(1-t)

lerp = (s,e,t) -> return s*(1-t)+e*t

distance=(x1,y1,x2,y2)->
	dx=x1-x2
	dy=y1-y2
	return math.sqrt(dx*dx+dy*dy)

class Ball
	@D=40
	@SIZE=16
	@COLRADIUS=12
	@LERP=8

	get_pos:(x,y)=>return CENTERX+x*@@D,CENTERY+y*@@D

	new:=>
		@sx,@sy=@get_pos(0,0)
		@ex,@ey=@sx,@sy
		@x,@y=@sx,@sy
		@lerpcnt=0

	get_target:=>
		tx=0
		ty=0
		if btn(0)
			ty=-1
		else if btn(1)
			ty=1
		else
			ty=0
		if btn(2)
			tx=-1
		else if btn(3)
			tx=1
		else
			tx=0
		d=distance(tx,ty,0,0)
		if d > 0
			tx/=d
			ty/=d
		return @get_pos(tx,ty)

	update:=>
		xnew,ynew=@get_target!
		if @ex!=xnew or @ey!=ynew
			@lerpcnt=0
			@sx,@sy=@x,@y
			@ex,@ey=xnew,ynew
		if @lerpcnt<@@LERP
			@lerpcnt+=1
			t=outquad(@lerpcnt/@@LERP)
			@x=lerp(@sx,@ex,t)
			@y=lerp(@sy,@ey,t)

	draw:=>
		spr(256,@x-@@SIZE,@y-@@SIZE,0,1,0,0,4,4)

class Hand
	@SIZE=16
	@COLRADIUS=12

	new:(x,y,dx,dy)=>
		@x=x
		@y=y
		@dx=dx
		@dy=dy

	update:=>
		@x+=@dx
		@y+=@dy

	isout:=>return @x+@@SIZE<0 or @x-@@SIZE>WIDTH or @y+@@SIZE<0 or @y-@@SIZE>HEIGHT

	draw:=>
		spr(320,@x-@@SIZE,@y-@@SIZE,0,1,0,0,4,4)

	onball:(ball)=>return distance(@x,@y,ball.x,ball.y)<@@COLRADIUS+ball.__class.COLRADIUS

