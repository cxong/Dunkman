export TIC=->

	ball\update!
	for hand in *hands
		hand\update!
		if hand\onball(ball)
			sfx(SFXHIT)
	hands=[hand for hand in *hands when not hand\isout! and not hand\onball(ball)]

	cls(0)

	-- draw image
	val_i=0
	run=0
	for y=0,136-1
		for x=0,240-1
			if run==0
				val_i=val_i+1
				run=img_runs[val_i]
