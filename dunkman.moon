class Speedline
	new:(y,l,c)=>
		@y=y
		@l=l
		@x=WIDTH
		@c=c

	update:(dx)=>
		@x+=dx

	alive:=>return @x+@l>0

	draw:=>
		tri(@x,@y,@x+@l,@y+1,@x+@l,@y-1,@c)
		tri(@x+@l*2,@y,@x+@l,@y+1,@x+@l,@y-1,@c)

class Speedlines
	new:=>
		@lines={}

	update:=>
		for i=1,2
			table.insert(@lines,Speedline(math.random(HEIGHT),WIDTH*2+math.random(50),math.random(4)))
		for l in *@lines
			l\update(-50)
		@lines=[l for l in *@lines when l\alive!]

	draw:=>
		cls(15)
		dy=15
		for i=1,4
			rect(0,dy*i,WIDTH,HEIGHT-dy*i*2,i)
		for l in *@lines
			l\draw!

class State
	new:=>
		@nextstate=self

	reset:=>return

	finish:=>return

	next:=>return self

	update:=>return

	draw:=>return

class SkipState extends State
	next:=>
		if btnp(0) or btnp(1) or btnp(2) or btnp(3) or btnp(4) or btnp(5)
			@finish!
			@nextstate\reset!
			return @nextstate
		return self

class TitleState extends SkipState
	draw:=>
		cls(0)
		drawimage(title_values,title_runs,WIDTH,HEIGHT,0,0)

class TimedState extends State
	new:(len)=>
		super!
		@tt=0
		@len=len

	reset:=>
		@tt=0

	update:=>
		@tt+=1

	next:=>
		if @tt>=@len
			@finish!
			@nextstate\reset!
			return @nextstate
		return self

class SlamState extends TimedState
	new:=>
		super 1000

